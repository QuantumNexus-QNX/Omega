{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "trivector-ai-monorepo",
  "builds": [
    {
      "src": "landing/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "landing/dist/spa"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/landing/$1"
    }
  ],
  "github": {
    "silent": true
  }
}

